#!/bin/bash

ulimit -t 1
echo $1 $2 $3 $4 $5 >> testruns.txt
case $2 in
  p1) $1 "6 -1 3 6 10" | diff 1.exp - && exit 0 ;;

  s) # single-valued fitness
  let fit=0
  $1 "6 -1 3 6 10" | diff 1.exp - && let fit=$fit+1
  let passed_all="$fit >= 1"
  echo $fit > $5
  if [ $passed_all -eq 1 ] ; then
    exit 0
  else
    exit 1
  fi ;;


esac
exit 1
